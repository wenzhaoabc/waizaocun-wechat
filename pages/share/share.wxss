/* pages/share/share.wxss */
#share_add{
  position: fixed;
  bottom: 10px;
  width: 100%;
  height: 80px;
  line-height: 80px;
  font-size: 60px;
  text-align: center;
  color: black;
}
.over_ignore{
  display: -webkit-box;  /*设置为弹性盒子*/
  -webkit-line-clamp: 2;  /*最多显示5行*/
  overflow: hidden;  /*超出隐藏*/
  text-overflow: ellipsis;  /*超出显示为省略号*/
  -webkit-box-orient: vertical;
  word-break: break-all;  /*强制英文单词自动换行*/
}